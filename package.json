  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/preset-env": "^7.20.2",
    "@jest/globals": "^29.4.2",
    "@pondevelopment/ponstandard": "^2.0.0",
    "babel-jest": "^29.4.2",
    "eslint": "^8.33.0",
    "esm": "^3.2.25",
    "jest": "^29.4.2",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "jscpd": "^3.5.3",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3"
  },
  "type": "module",
  "engines": {
    "node": ">= 18.13"
  },
  "dependencies": {
    "@hckrnews/error": "^1.0.4",
    "@hckrnews/express-callback": "^4.1.1",
    "@hckrnews/validator": "^8.0.5",
    "@ponbike/logger": "^0.2.2",
    "@ponbike/openapi-routes": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "openapi-backend": "^5.6.2",
    "swagger-ui-express": "^4.6.0"
  },
  "ponstandard": {
    "parser": "@babel/eslint-parser",
    "globals": [
      "btoa"
    ],
    "ignore": [
      "**/__fixtures__/"
    ]
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "settings": {
      "jsdoc": {
        "mode": "typescript"
      }
    },
    "extends": "./node_modules/@pondevelopment/ponstandard/eslintrc.json"
  }
